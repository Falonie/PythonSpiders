mongodump --archive=zdao_总经理_filter_duplicates.gz --gzip --db Falonie --collection zdao_总经理_filter_duplicates

mongorestore --gzip --archive=zdao_总经理_filter_duplicates.gz --db Falonie

mongoexport --db Falonie --collection zdao_股东_filter_duplicates --type=csv --fields url --out /home/salesmind/mind-ai-data/python/trunk/FalonieRepository/zaodao/zdao_创始人_filter_duplicates.csv

